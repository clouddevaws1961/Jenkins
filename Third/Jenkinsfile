pipeline
{
    agent any
    stages{
        stage('Clone repository')
        {
            echo "Clone repositiory started "    
            git https://github.com/clouddevaws1961/Jenkins.git
            checkout scm
        }
        stage ('build')
        {
            agent none
            steps {
                docker info
                docker build -t sandy1961/sandydockerimage_1961:1
                docker push
                
            }
        }
    }
}